<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <FrameLayout
        android:background="@android:color/transparent"
        android:layout_centerInParent="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.cardview.widget.CardView
            android:layout_marginLeft="@dimen/dp_30"
            android:layout_marginRight="@dimen/dp_30"
            android:layout_marginTop="75dp"
            app:contentPadding="@dimen/dp_5"
            app:cardCornerRadius="@dimen/dp_10"
            app:cardElevation="0dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:paddingLeft="@dimen/dp_10"
                android:paddingRight="@dimen/dp_10"
                android:layout_marginTop="90dp"
                android:paddingBottom="@dimen/dp_30"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/text1"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentLeft="true"
                        android:textSize="@dimen/dp_14"
                        android:textColor="#333333"
                        android:text="检核时间："
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="dsdsdsd"
                       android:layout_alignParentTop="true"
                        android:layout_toRightOf="@+id/text1"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:textSize="@dimen/dp_14"
                        android:textColor="#333333"
                        android:id="@+id/tv_start_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="dsdsdsd"
                        android:layout_marginTop="@dimen/dp_5"
                        android:layout_toRightOf="@+id/text1"
                        android:layout_below="@+id/tv_start_time"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:textSize="@dimen/dp_14"
                        android:textColor="#333333"
                        android:id="@+id/tv_end_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <RelativeLayout
                    android:padding="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/text2"
                        android:textSize="@dimen/dp_14"
                        android:textColor="#333333"
                        android:text="检核人员："
                        android:layout_alignParentLeft="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_user_name"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:layout_toRightOf="@+id/text2"
                        android:textSize="@dimen/dp_14"
                        android:textColor="#333333"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>

                <RelativeLayout
                    android:padding="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/text3"
                        android:textSize="@dimen/dp_14"
                        android:textColor="#333333"
                        android:text="检核状态："
                        android:layout_alignParentLeft="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_project_prograss"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:layout_toRightOf="@+id/text3"
                        android:textSize="@dimen/dp_14"
                        android:textColor="#333333"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>

                <RelativeLayout
                    android:padding="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/text4"
                        android:textSize="@dimen/dp_14"
                        android:textColor="#333333"
                        android:text="上传状态："
                        android:layout_alignParentLeft="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/tv_upload_status"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:layout_toRightOf="@+id/text4"
                        android:textSize="@dimen/dp_14"
                        android:textColor="#333333"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <TextView
                    android:id="@+id/tv_submit"
                    android:gravity="center"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/dp_14"
                    android:text="确定"
                    android:layout_marginLeft="25dp"
                    android:layout_marginRight="25dp"
                    android:padding="@dimen/dp_10"
                    android:background="@drawable/bg_blue_white_frame_background"
                    android:layout_marginTop="@dimen/dp_20"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </androidx.cardview.widget.CardView>
        <ImageView
            android:layout_gravity="center_horizontal"
            android:src="@mipmap/complete"
            android:layout_width="150dp"
            android:layout_height="150dp" />
    </FrameLayout>

</RelativeLayout>
